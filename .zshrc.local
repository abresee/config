# my zsh customizations

# aliases
alias pacman='pacman-color '
alias sudo='sudo '
alias vim='/home/allie/vim/bin/vim '

# env variables
export BOOSTDIR=~/boost
export LD_LIBRARY_PATH=$BOOSTDIR/lib
export SSH_ASKPASS=
export VISUAL=vim
export WORKON_HOME=$HOME/.virtualenvs
export PROJECT_HOME=$HOME/Devel
source /usr/bin/virtualenvwrapper.sh
if [ -n "$DISPLAY" ]; then
    BROWSER=firefox
else
    BROWSER=links
fi
